% --- Project Euler Problem 40 --- %
\begin{tcbProblem}[label={problem:0040:question:Eng}]
	An irrational decimal fraction is created by concatenating the positive integers:
	\begin{equation*}
		0.12345678910\textcolor{cudred}{1}112131415161718192021 \unicodecdots
	\end{equation*}
	It can be seen that the 12th digit of the fractional part is 1.

	If dn represents the nth digit of the fractional part, find the value of the following expression.
	\begin{equation*}
		{d}_{1} \times {d}_{10} \times {d}_{100} \times {d}_{1000} \times {d}_{10000} \times {d}_{100000} \times {d}_{1000000}
	\end{equation*}
\end{tcbProblem}
