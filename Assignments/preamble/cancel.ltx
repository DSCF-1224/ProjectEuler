% <cancel> package
% Require <xcolor> package and 'xcolor.ltx'
% 
\newcommand{\AccentCancel}[2]{\textcolor{AccentColor}{\cancel{\textcolor{#1}{#2}}}}
\newcommand{\AccentCancelto}[3]{\textcolor{AccentColor}{\cancelto{\textcolor{AccentColor}{#2}}{\textcolor{#1}{#3}}}}
\newcommand{\MainCancelto}[3]{\textcolor{MainColor}{\cancelto{\textcolor{MainColor}{#2}}{\textcolor{#1}{#3}}}}
\newcommand{\MainCancel}[2]{\textcolor{MainColor}{\cancel{\textcolor{#1}{#2}}}}
